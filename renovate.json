{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchManagers": ["dockerfile"],
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    }
  ],
  "dependencyDashboard": true,
  "ignorePaths": [],
  "internalChecksFilter": "strict",
  "separateMultipleMajor": true
}
